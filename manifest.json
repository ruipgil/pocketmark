{
	"name": "Pocketmark",
	"version": "0.2",
	"description": "From pocket to chrome bookmarks",
	"background": {
		"scripts": [
			"./libs/async.js",
			"./src/pocket.js",
			"./src/storage.js",
			"./src/background.js"
		],
		"persistent": false
	},
	"options_page": "./src/settings.html",
	"permissions": [
		"https://getpocket.com/v3/get",
		"bookmarks",
		"alarms",
		"identity"
	],
	"manifest_version": 2
}