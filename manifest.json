{
	"name": "Pocketmark",
	"version": "0.3.6",
	"description": "From pocket to chrome bookmarks",
	"background": {
		"scripts": [
			"./bower_components/async/lib/async.js",
			"./src/consumer.key.js",
			"./bower_components/pocketapi/pocket.js",
			"./src/storage.js",
			"./src/background.js"
		],
		"persistent": false
	},
	"icons": {
		"8": "resources/icon-8.png",
		"16": "resources/icon-16.png",
		"32": "resources/icon-32.png",
		"64": "resources/icon-64.png",
		"128": "resources/icon-128.png"
	},
	"options_page": "./src/settings.html",
	"permissions": [
		"https://getpocket.com/v3/get",
		"bookmarks",
		"storage",
		"alarms",
		"identity"
	],
	"manifest_version": 2
}