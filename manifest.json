{
	"name": "Pocketmark",
	"version": "0.1",
	"description": "From pocket to chrome bookmarks",
	"background": {
		"scripts": [
			"async.js",
			"pocket.js",
			"background.js"
		]
	},
	"permissions": [
		"https://getpocket.com/v3/get",
		"http://*/*",
		"bookmarks",
		"identity"
	],

	"manifest_version": 2
}