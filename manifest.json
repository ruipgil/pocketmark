{
	"name": "Pocketmark",
	"version": "0.3.3",
	"description": "From pocket to chrome bookmarks",
	"background": {
		"scripts": [
			"./bower_components/async/lib/async.js",
			"./src/consumer.key.js",
			"./bower_components/pocketapi/pocket.js",
			"./src/storage.js",
			"./src/background.js"
		],
		"persistent": false
	},
	"options_page": "./src/settings.html",
	"permissions": [
		"https://getpocket.com/v3/get",
		"bookmarks",
		"storage",
		"alarms",
		"identity"
	],
	"manifest_version": 2
}