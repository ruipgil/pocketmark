{
	"name": "Pocketmark",
	"version": "0.3.6",
	"description": "From pocket to chrome bookmarks",
	"background": {
		"scripts": [
			"./libs/async.js",
			"./js/consumer.key.js",
			"./libs/pocket.js",
			"./js/storage.js",
			"./js/background.js"
		],
		"persistent": false
	},
	"icons": {
		"8": "./img/icon-8.png",
		"16": "./img/icon-16.png",
		"32": "./img/icon-32.png",
		"64": "./img/icon-64.png",
		"128": "./img/icon-128.png"
	},
	"options_page": "./settings.html",
	"permissions": [
		"https://getpocket.com/v3/get",
		"bookmarks",
		"storage",
		"alarms",
		"identity"
	],
	"manifest_version": 2
}